[user]
  name = Eduardo Cereto
  email = 

[github]
  user = eduardocereto
  token = token

[core]
  editor = vim
  whitespace = fix,-indent-with-non-tab,trailing-space,cr-at-eol
  
[color]
  ui = auto

[color "branch"]
  current = yellow bold
  local = green bold
  remote = cyan bold

[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
  whitespace = red reverse

[color "status"]
  added = green bold
  changed = yellow bold
  untracked = red bold

[alias]
  st = status
  ci = commit
  br = branch
  co = checkout
  df = diff
  lg = log -p
  tree = log --graph --decorate --pretty=oneline --abbrev-commit
  treea = log --graph --decorate --pretty=oneline --abbrev-commit --all
  ls = ls-files
